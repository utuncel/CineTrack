<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml"
  prefHeight="800.0"
  prefWidth="1200.0" stylesheets="@DataImporterStyle.css" fx:controller="org.com.controller.dataimport.DataImporterController">
  <center>
    <VBox alignment="CENTER" spacing="20" styleClass="vbox">
      <VBox alignment="CENTER" spacing="15" styleClass="import-area">
        <Button text="Import CSV File" fx:id="chooseFileButton"/>
      </VBox>
    </VBox>
  </center>

  <left>
    <fx:include source="../sidebar/SidebarView.fxml"/>
  </left>
</BorderPane>