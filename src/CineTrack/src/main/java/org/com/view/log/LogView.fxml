<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml" prefHeight="800.0"
  prefWidth="1200.0"
  stylesheets="@LogStyle.css" xmlns="http://javafx.com/javafx" fx:controller="org.com.controller.log.LogController">

  <center>
    <TableView fx:id="logTableView">
      <columns>
        <TableColumn text="Timestamp" fx:id="timestampColumn"/>
        <TableColumn text="Level" fx:id="levelColumn"/>
        <TableColumn text="Message" fx:id="messageColumn"/>
      </columns>
    </TableView>
  </center>
  <left>
    <fx:include source="../sidebar/SidebarView.fxml"/>
  </left>
</BorderPane>